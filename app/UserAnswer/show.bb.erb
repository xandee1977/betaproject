<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @useranswer.user_id %></h1>
    <%= link_to "Edit", :action => :edit, :id => @useranswer.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">UserId</td>
        <td class="itemValue"><%= @useranswer.user_id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Answer</td>
        <td class="itemValue"><%= @useranswer.answer%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">TimeControl</td>
        <td class="itemValue"><%= @useranswer.time_control%></td>
      </tr>
      
    </table>
  </div>

</div>
