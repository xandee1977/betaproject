<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @userscore.user_id %></h1>
    <%= link_to "Edit", :action => :edit, :id => @userscore.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">UserId</td>
        <td class="itemValue"><%= @userscore.user_id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Score</td>
        <td class="itemValue"><%= @userscore.score%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">TimeControl</td>
        <td class="itemValue"><%= @userscore.time_control%></td>
      </tr>
      
    </table>
  </div>

</div>
