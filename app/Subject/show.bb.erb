<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @subject.theme_id %></h1>
    <%= link_to "Edit", :action => :edit, :id => @subject.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">ThemeId</td>
        <td class="itemValue"><%= @subject.theme_id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Title</td>
        <td class="itemValue"><%= @subject.title%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Description</td>
        <td class="itemValue"><%= @subject.description%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">TimeControl</td>
        <td class="itemValue"><%= @subject.time_control%></td>
      </tr>
      
    </table>
  </div>

</div>
